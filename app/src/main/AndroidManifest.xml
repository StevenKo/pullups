<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kosbrother.pullups" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".IndexActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".statistic.StatisticActivity"
            android:label="@string/title_activity_statistic" >
        </activity>
        <activity
            android:name=".training_program.TrainingProgramActivity"
            android:label="@string/title_activity_training_program" >
        </activity>
        <activity
            android:name=".pic_activities.warmups.WarmupsActivity"
            android:label="@string/title_activity_warmups" >
        </activity>
        <activity
            android:name=".pic_activities.pullups_type.PullupsTypesActivity"
            android:label="@string/title_activity_pullups_types" >
        </activity>
        <activity
            android:name=".pic_activities.auxiliary.AuxiliaryMovementsActivity"
            android:label="@string/title_activity_auxiliary_movements" >
        </activity>
        <activity
            android:name=".pic_activities.muscle_graph.MuscleGraphActivity"
            android:label="@string/title_activity_muscle_graph" >
        </activity>
        <activity
            android:name=".nutrition.NutritionActivity"
            android:label="@string/title_activity_nutrition" >
        </activity>
        <activity
            android:name=".alert_setting.AlertSettingActivity"
            android:label="@string/title_activity_alert_setting" >
        </activity>
        <activity
            android:name=".go_pro.GoProActivity"
            android:label="@string/title_activity_go_pro" >
        </activity>
        <activity
            android:name=".training_session.TraingingActivity"
            android:label="@string/title_activity_trainging" >
        </activity>

        <receiver
            android:name=".alert_setting.AlarmReceiver"
            android:enabled="true"
            android:exported="true" >
        </receiver>
    </application>

</manifest>
